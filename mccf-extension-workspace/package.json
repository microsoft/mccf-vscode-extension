{
  "name": "mccf-vscode-extension",
  "displayName": "mccf-vscode-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mccf-vscode-extension.helloWorld",
        "title": "MCCF: Hello World"
      },
      {
        "command": "mccf-vscode-extension.install",
        "title": "MCCF: Start Environment"
      },
      {
        "command": "mccf-vscode-extension.javascriptapp",
        "title": "MCCF: Start JavaScript CCF App"
      },
      {
        "command": "mccf-vscode-extension.cplusplusapp",
        "title": "MCCF: Create CCF C++ App Template"
      },
      {
        "command": "mccf-vscode-extension.startNetwork",
        "title": "MCCF: Start CCF Network"
      },
      {
        "command": "mccf-vscode-extension.addUser",
        "title": "MCCF: Add User"
